{
  "parser": "@typescript-eslint/parser",
  "extends": ["prettier", "plugin:node/recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {}
}
